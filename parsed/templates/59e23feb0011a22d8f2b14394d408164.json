{"components/search/show-more":"{{#if product_results}}\n    {{> components/faceted-search/show-more-facets product_results}}\n{{/if}}\n","components/faceted-search/show-more-facets":"<div class=\"modal-body\">\n    <h1 class=\"facet-quick-heading\">\n        {{all_facets}}\n    </h1>\n    <div class=\"form-field\" id=\"facetedSearch-filterItems\">\n        <input class=\"form-input\" type=\"search\" placeholder={{lang 'forms.search.search'}}>\n    </div>\n    <ul class=\"navList facetedSearch-optionColumns\">\n        {{#each facets}}\n            {{#if facet '===' ../all_facets}}\n                {{#each items}}\n                    <li class=\"navList-item\"><a\n                        href=\"{{url}}\"\n                        class=\"navList-action {{#if selected }} is-active {{/if}}\"\n                        rel=\"nofollow\"\n                        data-id=\"{{ id }}\"\n                        data-faceted-search-facet>{{title}} ({{count}})</a></li>\n                {{/each}}\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"}
